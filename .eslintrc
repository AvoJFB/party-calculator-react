{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "import/extensions": 0,
    "react/prop-types": 0,
    "no-template-curly-in-string": 0,
    "class-methods-use-this": 0,
    "linebreak-style": 0,
    "max-len": [
      "error",
      150
    ]
  },
  "plugins": [
    "flowtype"
  ],
  "globals": {
    "document": true
  }
}